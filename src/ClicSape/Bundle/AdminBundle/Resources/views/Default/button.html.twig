{# empty Twig template #}
{% macro edit(rout,id) %}
    <a class="btn btn-primary" href="{{path(rout, {'id': id}) }}">
                                <span class="glyphicon glyphicon-pencil" aria-hidden="true" ></span>
                            </a>
{% endmacro %}
{% macro delete(entity,id) %}
    <button  id="delete" type="button" class="btn btn-danger" data-entity="{{entity}}_{{id}}">
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
    </button>
{% endmacro %}